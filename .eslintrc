{
  "extends": "airbnb",
  "rules": {
    "no-multiple-empty-lines": [2, {"max": 2, "maxEOF": 2}],
    "import/no-extraneous-dependencies": 0,
    "comma-dangle": 0,
    "yoda": 0,
    "class-methods-use-this": 0,
    "no-underscore-dangle": 0,
    "no-console": 0,
    "no-plusplus": 0,
    "no-restricted-syntax": 0
  },
  "env": {
    "browser": true
  }
}
